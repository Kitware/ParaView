    .m1 add cascade -menu .m1.help
    menu .m2
    .m2 add cascade -menu .m2.help
    . configure -menu .m1
    raise .
    update
    list [catch {menu .m2.help} msg] $msg [. configure -menu ""] [destroy .m1] [destroy .m2]
} {0 .m2.help {} {} {}}
