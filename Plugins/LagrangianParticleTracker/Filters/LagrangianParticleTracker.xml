  <ProxyGroup name="filters">
    <SourceProxy class="vtkLagrangianParticleTracker"
                 label="Lagrangian Particle Tracker"
                 name="LagrangianParticleTracker">
      <InputProperty clean_command="RemoveAllInputs"
                     command="AddInputConnection"
                     name="FlowInput"
                     label="Flow Input">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain composite_data_supported="1"
                        name="input_type">
          <DataType value="vtkDataSet" />
        </DataTypeDomain>
        <InputArrayDomain name="input_array"
                          number_of_components="3" />
        <Documentation>This property specifies the flow input to the
        Lagrangian Particle Tracker filter.</Documentation>
      </InputProperty>
      <InputProperty command="SetSourceConnection"
                     label="Particle Seeds"
                     name="ParticleSeeds">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain composite_data_supported="1"
                        name="input_type">
          <DataType value="vtkDataSet" />
        </DataTypeDomain>
        <InputArrayDomain attribute_type="point"
                          name="source_array_3"
                          number_of_components="3" />
        <InputArrayDomain attribute_type="point"
                          name="source_array_1"
                          number_of_components="1" />
        <Documentation>This property specifies the input used to obtain the
        seed points to generate particles.</Documentation>
      </InputProperty>
      <InputProperty command="SetSurfaceConnection"
                     label="Surface"
                     name="Surface">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain composite_data_supported="1"
                        name="input_type">
          <DataType value="vtkDataSet" />
        </DataTypeDomain>
        <InputArrayDomain attribute_type="field"
                          name="input_array"
                          number_of_components="1" />
        <Documentation>This property specifies the input used to surfaces the particles will interact with.
        </Documentation>
      </InputProperty>
      <OutputPort name="ParticlePaths" index="0" />
      <OutputPort name="ParticleInteractions" index="1" />
      <ProxyProperty command="SetIntegrationModel"
                     label="Integration Model"
                     name="IntegrationModel"
                     panel_visibility="default">
        <ProxyListDomain name="proxy_list">
          <Group name="lagrangian_integration_models"/>
        </ProxyListDomain>
      </ProxyProperty>
      <ProxyProperty command="SetIntegrator"
                     name="Integrator">
        <ProxyListDomain name="proxy_list">
          <Group name="integrators"/>
        </ProxyListDomain>
        <Documentation>This property determines which integrator (with
        increasing accuracy) to use for integrating particles.</Documentation>
      </ProxyProperty>
      <IntVectorProperty command="SetCellLengthComputationMode"
                         default_values="3"
                         name="CellLengthComputationMode"
                         number_of_elements="1">
        <EnumerationDomain name="enum">
          <Entry text="Last Cell Length"
                 value="0" />
          <Entry text="Current Cell Length"
                 value="1" />
          <Entry text="Last Cell Velocity Direction"
                 value="2" />
          <Entry text="Current Cell Velocity Direction"
                 value="3" />
          <Entry text="Last Cell Divergence Theorem"
                 value="4" />
          <Entry text="Current Cell Divergence Theorem"
                 value="5" />
        </EnumerationDomain>
        <Documentation>This property controls how the filter computes a cell length
        to generate Step value for the integrator using the StepFactor.
        The current default method is "Current Cell Velocity Direction" but it
        is not the fastest and may not work with very irregular mesh.
        "Current Cell Divergence Theorem" is great for irregular mesh but it is
        even more slow and it does not support Voxel cell types.
        "Cell Length" based computation is fast but will work
        only with non-elongated cell.</Documentation>
      </IntVectorProperty>

      <DoubleVectorProperty command="SetStepFactor"
                            default_values="1.0"
                            name="Step"
                            number_of_elements="1">
        <Documentation>This property contains the step in cell unit
        that the integration should take at each integration.</Documentation>
      </DoubleVectorProperty>
      <DoubleVectorProperty command="SetStepFactorMin"
                            default_values="0.9"
                            name="MinStep"
                            number_of_elements="1"
                            panel_visibility="advanced" >
        <Documentation>This property contains the minimum step in cell unit
        that the integration should take at each integration with a non small
        velocity.</Documentation>
      </DoubleVectorProperty>
      <DoubleVectorProperty command="SetStepFactorMax"
                            default_values="1.1"
                            name="MaxStep"
                            number_of_elements="1"
                            panel_visibility="advanced" >
        <Documentation>This property contains the maximum step in cell unit
        that the integration should take at each integration.</Documentation>
      </DoubleVectorProperty>
      <IntVectorProperty command="SetMaximumNumberOfSteps"
                            default_values="100"
                            name="NumberOfSteps"
                            number_of_elements="1">
        <Documentation>This property contains the absolute maximum number of steps
        a particle can integrate before being terminated. Increasing this property
        too much will cost time and memory during computation.
        </Documentation>
      </IntVectorProperty>
      <IntVectorProperty command="SetAdaptiveStepReintegration"
                         default_values="0"
                         name="AdaptiveStepReintegration"
                         number_of_elements="1"
                         panel_visibility="advanced" >
        <BooleanDomain name="bool" />
        <Documentation>If AdaptativeStepReintegration is true, the Lagrangian
        particle tracker will check the results of integration.
        If the distance between the current point and the next point is greater
        than StepFactorMax * cellLength, then the StepFactor will be decreased
        and integration will be tried again until the distance is within the bounds
        or the StepFactorMin limit the decrease of the StepFactor.
        </Documentation>
      </IntVectorProperty>
      <IntVectorProperty command="SetGenerateParticlePathsOutput"
                         default_values="1"
                         name="GenerateParticlePathsOutput"
                         number_of_elements="1"
                         panel_visibility="advanced" >
        <BooleanDomain name="bool" />
        <Documentation>If GenerateParticlePathsOutput is true, the tracker will generate the particle paths output.
        </Documentation>
      </IntVectorProperty>
      <!-- End LagrangianParticleTracker -->
    </SourceProxy>
    <SourceProxy name="LagrangianSurfaceHelper"
                 class="vtkLagrangianSurfaceHelper"
                 label="Lagrangian Surface Helper">
      <InputProperty command="SetInputConnection"
                     label="Surface Input"
                     name="SurfaceInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain composite_data_supported="1"
                        name="input_type">
          <DataType value="vtkDataSet" />
        </DataTypeDomain>
        <Documentation>This property specifies the input to generate surface data on.
        </Documentation>
      </InputProperty>
      <ProxyProperty command="SetIntegrationModel"
                     label="Integration Model"
                     disable_sub_trace="1"
                     name="IntegrationModel"
                     panel_visibility="default">
        <ProxyListDomain name="proxy_list">
          <Group name="lagrangian_integration_models"/>
        </ProxyListDomain>
        <Hints>
          <ProxyPropertyWidget selected_proxy_panel_visibility="never" />
        </Hints>
        <Documentation>This property specifies the integration model to use to generate data.
        </Documentation>
      </ProxyProperty>
      <StringVectorProperty animateable="1"
                            clean_command="RemoveAllArraysToGenerate"
                            command="SetArrayToGenerate"
                            name="ArrayToGenerate"
                            element_types="2 0 0 0 2"
                            number_of_elements_per_command="5"
                            repeat_command="1"
                            set_number_command="SetNumberOfArrayToGenerate"
                            use_index="1"
                            panel_widget="integration_model_surface_helper_widget">
        <Documentation>This property specifies arrays to generate in the output.
        </Documentation>
      </StringVectorProperty>
      <!-- End LagrangianSeedHelperBase -->
    </SourceProxy>
