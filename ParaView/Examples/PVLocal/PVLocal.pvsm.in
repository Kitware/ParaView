<ServerManagerConfiguration>
  <ProxyGroup name="sources">
   <SourceProxy name="LocalConeSource" class="vtkLocalConeSource">
      <IntVectorProperty
         name="Resolution"
         command="SetResolution"
         number_of_elements="1"
         default_values="6" >
        <IntRangeDomain name="range" min="0" max="512" />
      </IntVectorProperty>

      <DoubleVectorProperty
         name="Radius"
         command="SetRadius"
         number_of_elements="1"
         default_values="0.5" >
      </DoubleVectorProperty>

      <DoubleVectorProperty
         name="Height"
         command="SetHeight"
         number_of_elements="1"
         default_values="1.0" >
        <DoubleRangeDomain name="range" min="0" />
      </DoubleVectorProperty>

     <IntVectorProperty 
        name="Capping" 
        command="SetCapping" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain name="bool"/>
     </IntVectorProperty>
   <!-- End Cone -->
   </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
